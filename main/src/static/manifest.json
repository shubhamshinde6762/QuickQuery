{
  "manifest_version": 3,
  "name": "QuickQuery",
  "description": "Quick access to search queries",
  "version": "1.0.1",
  "action": {
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "background": {
    "service_worker": "background.js"
  },
  "side_panel": {
    "default_path": "sidePanel.html"
  },
  "permissions": ["sidePanel", "notifications", "storage", "tabs"]
}
